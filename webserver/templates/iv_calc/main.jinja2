<div class="container-fluid wrapper">
    {% block contents %}
        <h1>It works!</h1>

        <div class="row-fluid">
            <div class="span8 block">
                <h4>Pokemon:</h4>
                <select id="pokemon-select">
                    <option value="172">Pichu</option>
                    <option value="182">Belossom</option>
                    <option value="192">Sunflora</option>
                </select>
            </div>
            <div class="span4 pull-right">
                <img src="/public/vendor/veekun/sugimori/172.png" class="img-circle sprite" />
            </div>
        </div>

        <div class="row-fluid gap">
            <div class="span4 block"></div>
            <div class="span4 block"></div>
            <div class="span4 block"></div>
        </div>
        <div class="row-fluid">
            <div class="span4 block"></div>
            <div class="span4 block"></div>
            <div class="span4 block"></div>
        </div>

        <div class="row-fluid gap">
            <div class="span12 block">
                <table class="table table-compressed table-hover">
                    <thead>
                        <tr>
                            <th>LVL</th>
                            <th>HP</th>
                            <th>ATK</th>
                            <th>DEF</th>
                            <th>SpA</th>
                            <th>SpD</th>
                            <th>SPE</th>
                            <th><a class="btn btn-mini table-button" href="#"><i class="icon-remove-circle"></i> Clear</a></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><strong>14</strong></td>
                            <td>68</td>
                            <td>34</td>
                            <td>58 / 4</td>
                            <td>46</td>
                            <td>90</td>
                            <td>128</td>
                            <td><a class="btn btn-mini table-button" href="#"><i class="icon-remove"></i> Remove</a></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>

        <div class="row-fluid gap">
            <div class="span12 block">
                <div class="row-fluid">
                    <div class="input-prepend input-append span1">
                        <button class="btn stat-btn" type="button"><i class="icon-minus"></i></button>
                        <input type="text" class="stat-input" placeholder="LVL">
                        <button class="btn stat-btn" type="button"><i class="icon-plus"></i></button>
                    </div>
                    <div class="input-prepend input-append span1">
                        <button class="btn stat-btn" type="button"><i class="icon-minus"></i></button>
                        <input type="text" class="stat-input" placeholder="HP">
                        <button class="btn stat-btn" type="button">
                        <i class="icon-plus"></i></button>
                    </div>
                    <div class="input-prepend input-append span1">
                        <button class="btn stat-btn" type="button"><i class="icon-minus"></i></button>
                        <input type="text" class="stat-input" placeholder="ATK">
                        <button class="btn stat-btn" type="button"><i class="icon-plus"></i></button>
                    </div>
                    <div class="input-prepend input-append span1">
                        <button class="btn stat-btn" type="button"><i class="icon-minus"></i></button>
                        <input type="text" class="stat-input" placeholder="DEF">
                        <button class="btn stat-btn" type="button"><i class="icon-plus"></i></button>
                    </div>
                    <div class="input-prepend input-append span1">
                        <button class="btn stat-btn" type="button"><i class="icon-minus"></i></button>
                        <input type="text" class="stat-input" placeholder="SpA">
                        <button class="btn stat-btn" type="button"><i class="icon-plus"></i></button>
                    </div>
                    <div class="input-prepend input-append span1">
                        <button class="btn stat-btn" type="button"><i class="icon-minus"></i></button>
                        <input type="text" class="stat-input" placeholder="SpD">
                        <button class="btn stat-btn" type="button"><i class="icon-plus"></i></button>
                    </div>
                    <div class="input-prepend input-append span1">
                        <button class="btn stat-btn" type="button"><i class="icon-minus"></i></button>
                        <input type="text" class="stat-input" placeholder="SPE">
                        <button class="btn stat-btn" type="button"><i class="icon-plus"></i></button>
                    </div>
                </div>
            </div>
        </div>
    {% endblock contents %}
</div> <!-- /container -->

